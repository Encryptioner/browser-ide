{
  "name": "browser-ide-pro",
  "version": "2.0.0",
  "type": "module",
  "description": "Production-ready VS Code-like IDE in the browser with multi-LLM support",
  "author": "Browser IDE Team",
  "license": "MIT",
  "engines": {
    "node": "^22.16.0"
  },
  "bin": {
    "claude": "./bin/claude-simple.js"
  },
  "scripts": {
    "dev": "vite",
    "dev:mobile": "vite --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "preview:mobile": "vite preview --host 0.0.0.0",
    "deploy": "gh-pages -d dist",
    "deploy:script": "./scripts/deploy.sh",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules",
    "test": "echo 'No tests configured yet' && exit 0",
    "validate": "pnpm type-check && pnpm lint && pnpm build",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\""
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.55",
    "@anthropic-ai/sdk": "^0.71.0",
    "@isomorphic-git/lightning-fs": "^4.6.0",
    "@monaco-editor/react": "^4.6.0",
    "@tailwindcss/typography": "^0.5.19",
    "@vscode/codicons": "^0.0.35",
    "@webcontainer/api": "^1.1.9",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "buffer": "^6.0.3",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.6",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.7",
    "diff": "^8.0.2",
    "diff2html": "^3.4.52",
    "idb": "^8.0.0",
    "isomorphic-git": "^1.25.6",
    "jszip": "^3.10.1",
    "lucide-react": "^0.555.0",
    "monaco-editor": "^0.55.1",
    "nanoid": "^5.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^1.0.9",
    "react-router-dom": "^6.21.1",
    "react-syntax-highlighter": "^15.5.0",
    "sonner": "^2.0.7",
    "vscode-oniguruma": "^2.0.1",
    "vscode-textmate": "^9.2.1",
    "vscode-web": "^1.91.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "globals": "^16.5.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vscode-languageserver-protocol": "^3.17.5",
    "workbox-window": "^7.0.0"
  },
  "packageManager": "pnpm@8.14.0"
}
